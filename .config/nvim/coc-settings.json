{
    "coc.preferences.rootPatterns": [
      ".git",
      ".hg",
      ".projections.json",
      "package.json",
      "composer.json"
    ],
    "emmet.includeLanguages": {
        "vue-html": "html"
    },
    "eslint.autoFixOnSave": true,
    "eslint.run":"onType",
    "eslint.filetypes": ["vue","javascript","typescript"],
    "tslint.run":"onType",
    "tslint.autoFixOnSave": true,
    "tailwindCSS.headwind.runOnSave": false,
    "coc.preferences.formatOnSaveFiletypes": ["go","php"]
}
